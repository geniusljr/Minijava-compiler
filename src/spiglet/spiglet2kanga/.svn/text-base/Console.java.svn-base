package spiglet.spiglet2kanga;

/**
 * 控制台输出类
 * @author GeniusLJR
 *
 */
public class Console {
	private static String code = "";//存储翻译后kanga代码
	
	//所有寄存器名称
	public static final String[] regName={"s0","s1","s2","s3","s4","s5","s6","s7","t0","t1","t2","t3","t4","t5","t6","t7","t8","t9"};
	
	/**
	 * 把一段kanga代码添加到code中
	 * @param statement 一段kanga代码
	 */
	public static void Print(String statement) {
		code += statement;
	}
	
	/**
	 * 输出最后结果
	 */
	public static String ResultOutput() {
		return code;
	}
}
