package minijava.minijava2piglet;

public class Translate {
	//最终PIGLET代码
	private static String code="";
	//用来分配寄存器
	public static int temp_count = 20;
	//用来分配Label标号
	public static int label_count = 0;
	
	/**
	 * 把statement字符串加到code后面
	 * @param statement 要加入的字符串
	 */
	public static void Print(String statement) {
		code += statement;
	}
	/**
	 * 把最终PIGLET代码输出
	 */
	public static String ResultOutput() {
		return code;
	}
}
